{
  "name": "deltagraph",
  "version": "1.0.0",
  "main": "index.js",
  "license": "MIT",
  "scripts": {
    "start": "parcel src/index.html --no-cache",
    "test": "jest",
    "inspect-build": "node --inspect --inspect-brk ./node_modules/parcel-bundler/bin/cli ./src/index.html --no-cache",
    "test:updateSnapshots": "jest --updateSnapshot",
    "build": "parcel build src/index.html --no-cache --public-url ./ ",
    "eslint": "eslint ./src"
  },
  "dependencies": {
    "@cycle/http": "^12.0.0",
    "@cycle/run": "^1.0.0",
    "@cycle/time": "^0.7.1",
    "babel-plugin-dynamic-import-node": "^1.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-env": "^1.6.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "history": "^4.7.2",
    "parcel-bundler": "^1.4.1",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-dom": "^16.2.0",
    "react-redux": "^5.0.6",
    "react-router": "3.2.0",
    "react-router-redux": "^4.0.8",
    "redux": "^3.7.2",
    "redux-cycles": "^0.4.1",
    "semantic-ui-css": "^2.2.12",
    "semantic-ui-react": "^0.77.2",
    "styled-components": "^2.4.0",
    "xstream": "^10"
  },
  "devDependencies": {
    "babel-eslint": "^8.1.2",
    "enzyme": "^3.3.0",
    "enzyme-adapter-react-16": "^1.1.1",
    "enzyme-to-json": "^3.3.0",
    "eslint": "^4.14.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-config-standard": "^11.0.0-beta.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-node": "^5.2.1",
    "eslint-plugin-prettier": "^2.4.0",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-react": "^7.5.1",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^22.0.4",
    "prettier": "^1.10.2"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": ["html"],
    "snapshotSerializers": ["enzyme-to-json/serializer"],
    "setupTestFrameworkScriptFile": "<rootDir>/config/test.setup.js"
  }
}
